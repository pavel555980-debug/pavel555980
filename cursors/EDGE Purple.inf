[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%,%10%\%CUR_DIR%\%person%,%10%\%CUR_DIR%\%pin%"

[Scheme.Cur]
standby-purple.ani
help.ani
work.ani
cortana-purple.ani
text.cur
hal.ani
vertical.cur
horizontal.cur
diagonal1.cur
diagonal2.cur
move.cur
hover-purple.ani
pointer.cur
standby-purple-left.ani
person.cur
pin.cur

[Scheme.Txt]
Readme.pdf
Preview.png

[Strings]
CUR_DIR       = "Cursors\EDGE"
SCHEME_NAME   = "EDGE Purple"
pointer       = "standby-purple.ani"
help          = "help.ani"
work          = "work.ani"
busy          = "cortana-purple.ani"
text          = "text.cur"
unavailiable  = "hal.ani"
vert          = "vertical.cur"
horz          = "horizontal.cur"
dgn1          = "diagonal1.cur"
dgn2          = "diagonal2.cur"
move          = "move.cur"
link          = "hover-purple.ani"
cross         = "pointer.cur"
hand          = "pointer.cur"
alternate     = "standby-purple-left.ani"
person        = "person.cur"
pin           = "pin.cur"